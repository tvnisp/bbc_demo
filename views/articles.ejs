<%- include("./partials/header")%>

<section class="container pt-5" id="grow">
  <p class="count"></p>
  <article class="article">
    <!-- <h1 class="title"></h1>
    <h2 class="heading"></h2>
    <div class="img"></div>
    <div class="paragraph"></div>
    <div class="list"></div> -->
  </article>
  
  <button type="button" class="btn btn-secondary mt-3 mb-3 pr-3 pl-3 float-left" id="last-article">Back</button>
